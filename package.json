{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "A Discord bot powered by discord.js",
    "main": "dist/index.js",
    "type": "commonjs",
    "scripts": {
        "start": "node dist/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
