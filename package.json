        "openai": "^4.89.0",
        "qrcode": "^1.5.4",
        "sharp": "^0.33.5",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.24",
        "@types/qrcode": "^1.5.5",
        "@types/ws": "^8.18.0",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.3.2",
        "tsx": "^4.7.1",
        "typescript": "^5.3.3"
